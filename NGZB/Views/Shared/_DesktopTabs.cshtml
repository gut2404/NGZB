@*首页tabs*@
@using NGZB.Controllers
@using NGZB.Models.Object;
@{ var objTokenKey = SysController.__TokenKey(); }
@{
    if (ViewBag.Tabs != null)
    {
        foreach (var item in (List<DesktopTabs>)ViewBag.Tabs)
        {
            <div class="easyui-tabs" title="@item.TabsTitle" style="border:0px" data-options="fit:true,closable: true">
                <iframe scrolling="no" frameborder="0" src="@Url.Action(item.Active, item.Controller, objTokenKey)" style="width:100%;height:100%;border:0px;margin-top:2px;"></iframe>
            </div>
        }
    }
}